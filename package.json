{
  "name": "vue-path-store",
  "description": "A simple reactive store for Vue, that uses the dot notation path syntax",
  "version": "0.0.0-semantic-release",
  "author": {
    "name": "Giannis Koutsaftakis",
    "email": "koutsaftakis@gmail.com"
  },
  "license": "MIT",
  "repository": "https://github.com/kouts/vue-path-store",
  "keywords": [
    "vue",
    "state",
    "store",
    "path",
    "keypath",
    "dot"
  ],
  "main": "dist/cjs/pathStore.js",
  "module": "dist/es/pathStore.js",
  "unpkg": "dist/umd/pathStore.umd.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:vue": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "watch": "vuepress dev docs",
    "rollup": "rollup -c",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "build": "rollup -c",
    "lint": "eslint . --ext .js,.vue",
    "lint-fix": "eslint . --fix --ext .js,.vue"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/eslint-parser": "^7.13.14",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.1.4",
    "bootstrap": "^4.6.0",
    "core-js": "^3.12.1",
    "eslint": "^7.26.0",
    "eslint-config-kouts": "^1.3.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.9.0",
    "prettier": "2.2.1",
    "rollup": "^2.45.2",
    "rollup-plugin-terser": "^5.3.1",
    "vue": "^2.6.12",
    "vuepress": "^1.8.2"
  }
}
